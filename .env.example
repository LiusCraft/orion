# CDN Agent 环境变量配置

# 数据库配置
CDNAGENT_DATABASE_PASSWORD=your-database-password

# JWT密钥
CDNAGENT_JWT_SECRET=your-jwt-secret-key

# AI配置 - Claude API
CDNAGENT_AI_LLM_API_KEY=your-claude-api-key
CDNAGENT_AI_LLM_BASE_URL=https://your-third-party-api.com/v1

# 可选的其他AI配置
# CDNAGENT_AI_LLM_PROVIDER=claude
# CDNAGENT_AI_LLM_MODEL=claude-3-sonnet-20240229
# CDNAGENT_AI_LLM_TEMPERATURE=0.7
# CDNAGENT_AI_LLM_MAX_TOKENS=2000

# Redis配置 (可选)
# CDNAGENT_REDIS_PASSWORD=your-redis-password

# 工具集成 (可选)
# CDNAGENT_TOOLS_GRAFANA_API_KEY=your-grafana-api-key
# CDNAGENT_TOOLS_LOGS_PASSWORD=your-elasticsearch-password